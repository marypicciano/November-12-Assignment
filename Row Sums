def row_sums(my_matrix: list):
     # Iterate through each row of the matrix
    for row in my_matrix:
        # Calculate the sum of the row
        row_sum = sum(row)
        # Append the sum to the end of the row
        row.append(row_sum)
my_matrix = [
    [1, 2, 3],
    [4, 5, 6]
    ]
row_sums(my_matrix)
print(my_matrix)
