class Pet:
    def __init__(self, name: str, species: str, year_of_birth: int):
        # Initialize the attributes
        self.name = name
        self.species = species
        self.year_of_birth = year_of_birth

# Define the function new_pet that creates and returns a new Pet object
def new_pet(name: str, species: str, year_of_birth: int) -> Pet:
    return Pet(name, species, year_of_birth)
# Example usage of the new_pet function
fluffy = new_pet("Fluffy", "dog", 2024)

# Print the attributes of the fluffy pet object
print(fluffy.name)       
print(fluffy.species)
print(fluffy.year_of_birth) 
